{
	"width": 512,
	"height": 512,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"Enemy": {
			"type": "script",
			"path": "script/Enemy.js",
			"global": true
		},
		"EnemyBullet": {
			"type": "script",
			"path": "script/EnemyBullet.js",
			"global": true
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"Player": {
			"type": "script",
			"path": "script/Player.js",
			"global": true
		},
		"StateManager": {
			"type": "script",
			"path": "script/StateManager.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "2"
	}
}